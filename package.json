{
  "name": "cursor-card-helper",
  "version": "1.0.0",
  "description": "Professional credit card auto-fill tool with advanced Luhn algorithm and realistic data generation",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:firefox": "node scripts/build-scripts/build-firefox.js",
    "build:chrome": "node scripts/build-scripts/build-chrome.js",
    "build:all": "npm run build:firefox && npm run build:chrome",
    "package:chrome": "npm run build:chrome && node scripts/build-scripts/package-chrome.js",
    "package:firefox": "npm run build:firefox && node scripts/build-scripts/package-firefox.js",
    "package:all": "npm run package:chrome && npm run package:firefox",
    "firefox:sign": "node scripts/build-scripts/sign-firefox.js",
    "firefox:run": "web-ext run --source-dir=./build-firefox",
    "firefox:lint": "web-ext lint --source-dir=./build-firefox",
    "clean": "rm -rf build-chrome build-firefox dist coverage"
  },
  "keywords": [
    "credit-card",
    "testing",
    "luhn",
    "chrome-extension"
  ],
  "author": "Justin",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "archiver": "7.0.1",
    "eslint": "^8.57.0",
    "javascript-obfuscator": "^4.0.0",
    "jest": "^29.7.0",
    "terser": "5.44.0",
    "web-ext": "8.10.0",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1"
  }
}
